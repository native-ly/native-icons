{
  "name": "native-icons",
  "version": "1.0.0",
  "description": "Icon component for React Native & Expo with better font family management",
  "author": "Jakub Biesiada",
  "license": "MIT",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "release": "rimraf lib/* && npm run prettier && npm run lint && npm run build",
    "build": "tsc",
    "watch": "npm run build -- --watch",
    "lint": "eslint --fix 'src/**/*.{tsx,ts}'",
    "prettier": "prettier --write 'src/**/*.{tsx,ts}'"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/native-ly/native-icons.git"
  },
  "keywords": [
    "TypeScript",
    "React",
    "React Native",
    "icons"
  ],
  "bugs": {
    "url": "https://github.com/native-ly/native-icons/issues"
  },
  "homepage": "https://github.com/native-ly/native-icons#readme",
  "dependencies": {
    "react-native-vector-icons": "^6.6.0"
  },
  "devDependencies": {
    "@types/color": "^3.0.1",
    "@types/jest": "^25.2.3",
    "@types/react": "^16.9.35",
    "@types/react-native": "^0.62.12",
    "@types/react-native-vector-icons": "^6.4.5",
    "@typescript-eslint/parser": "^3.0.2",
    "eslint": "^7.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.20.0",
    "eslint-plugin-react-native": "^3.8.1",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.7",
    "prettier": "^2.0.5",
    "react": "^16.13.1",
    "react-native": "^0.62.1",
    "typescript": "^3.9.3"
  },
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-native": ">=0.59.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{tsx,ts}": [
      "npm run prettier",
      "npm run lint"
    ]
  }
}
