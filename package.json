{
  "name": "native-icons",
  "version": "1.1.0",
  "description": "Icon component for React Native & Expo with better font family management",
  "author": "Jakub Biesiada",
  "license": "MIT",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "//": "rimraf lib/* && npm run prettier && npm run lint && npm run build",
    "build": "tsc",
    "test": "jest --coverage",
    "watch": "npm run build -- --watch",
    "watch:test": "npm run test -- --watch",
    "lint": "eslint --fix 'src/**/*.{tsx,ts}'",
    "prettier": "prettier --write 'src/**/*.{tsx,ts}'"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/native-ly/native-icons.git"
  },
  "keywords": [
    "react-native",
    "expo",
    "vector",
    "icons",
    "library",
    "component"
  ],
  "bugs": {
    "url": "https://github.com/native-ly/native-icons/issues"
  },
  "homepage": "https://github.com/native-ly/native-icons#readme",
  "dependencies": {
    "react-native-vector-icons": "^7.0.0"
  },
  "devDependencies": {
    "@testing-library/jest-native": "^3.3.0",
    "@testing-library/react-native": "^7.0.2",
    "@types/jest": "^26.0.10",
    "@types/react": "^16.9.48",
    "@types/react-native": "^0.63.11",
    "@types/react-native-vector-icons": "^6.4.5",
    "@typescript-eslint/parser": "^3.10.1",
    "babel-jest": "^26.3.0",
    "eslint": "^7.7.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.6",
    "eslint-plugin-react-native": "^3.9.1",
    "husky": "^4.2.5",
    "jest": "^26.4.2",
    "lint-staged": "^10.2.13",
    "prettier": "^2.1.1",
    "react": "^16.13.1",
    "react-native": "^0.62.1",
    "react-test-renderer": "^16.13.1",
    "ts-jest": "^26.3.0",
    "typescript": "^4.0.2"
  },
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-native": ">=0.59.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{tsx,ts}": [
      "npm run prettier",
      "npm run lint"
    ]
  }
}
